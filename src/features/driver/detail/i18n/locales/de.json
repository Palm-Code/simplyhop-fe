        "items": [
          {
            "id": "whatsapp",
            "icon": {
              "name": "Whatsapp"
            },
            "href": "https://wa.me/?text={{message}}"
          },
          {
            "id": "facebook",
            "icon": {
              "name": "Facebook2"
            },
            "href": "https://www.facebook.com/sharer/sharer.php?u={{link}}"
          },
          {
            "id": "twitter",
            "icon": {
              "name": "Twitter"
            },
            "href": "https://twitter.com/intent/tweet?text={{message}}"
          },
          {
            "id": "telegram",
            "icon": {
              "name": "Telegram"
            },
            "href": "https://telegram.me/share/url?text={{message}}"
          },
          {
            "id": "thread",
            "icon": {
              "name": "Thread"
            },
            "href": "https://threads.net/intent/post?text={{link}}"
          }
        ],
        "cta": {
          "copy": {
            "children": "Link kopieren"
          }
        }
      }
    }
